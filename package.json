{
  "name": "integration-test",
  "version": "1.0.0",
  "description": "NodeJS integration API built as techincal challenge for LinkAPI",
  "main": "app.js",
  "scripts": {
    "start": "npm-run-all -s deleteBuild build server",
    "dev": "nodemon --exec babel-node src/app.js",
    "server": "node build/app.js",
    "build": "babel src --out-dir build --copy-files",
    "deleteBuild": "rm -rf build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/unimatrix2/node-integration-test.git"
  },
  "keywords": [
    "technical",
    "test",
    "nodejs",
    "express"
  ],
  "author": "Danny Nisenbaum (unimatrix2)",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.16",
    "@babel/core": "^7.12.16",
    "@babel/node": "^7.12.16",
    "@babel/preset-env": "^7.12.16",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.4.1",
    "mongoose": "^5.11.15",
    "npm-run-all": "^4.1.5"
  },
  "devDependencies": {
    "eslint": "^7.20.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.7"
  }
}
